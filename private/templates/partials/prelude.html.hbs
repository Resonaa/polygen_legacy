<script media="screen and (min-width: 576px)">
    paceOptions = {
        maxProgressPerFrame: 40,
        ajax: {
            trackMethods: ["GET", "POST", "PUT", "DELETE"],
            trackWebSockets: true,
            ignoreURLs: []
        }
    };
</script>
<script media="screen and (min-width: 576px)" src="https://cdn.staticfile.org/pace/1.2.4/pace.min.js"></script>
<link media="screen and (min-width: 576px)" rel="stylesheet"
    href="https://cdn.staticfile.org/pace/1.2.4/themes/blue/pace-theme-minimal.min.css">

<meta name="viewport" content="width=device-width, initial-scale=1, maximum-scale=1, user-scalable=no">

<link rel="stylesheet" href="https://cdn.staticfile.org/font-awesome/5.15.4/css/fontawesome.min.css">

<link href="https://cdn.staticfile.org/twitter-bootstrap/5.1.1/css/bootstrap.min.css" rel="stylesheet">
<script src="https://cdn.staticfile.org/twitter-bootstrap/5.1.1/js/bootstrap.bundle.min.js"></script>

<link href="/css/index.min.css" rel="stylesheet">

<script src="https://cdn.staticfile.org/jquery/1.12.4/jquery.min.js"></script>

<script src="https://cdn.staticfile.org/sweetalert/2.1.2/sweetalert.min.js"></script>

<script src="/js/algorithm.min.js"></script>

{{#if username}}
<script>
    if (!userExists("{{username}}")) {
        $.post('/logout', {}, _ => window.location.href = '/login');
    }
</script>
{{/if}}